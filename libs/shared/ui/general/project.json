{
  "name": "shared-ui-general",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/ui/general/src",
  "prefix": "ui",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shared/ui/general/ng-package.json",
        "tailwindConfig": "libs/shared/ui/general/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/ui/general/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/ui/general/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/ui/general/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/shared/ui/general/.storybook",
        "browserTarget": "shared-ui-general:build-storybook",
        "compodoc": false,
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/shared-ui-general",
        "configDir": "libs/shared/ui/general/.storybook",
        "browserTarget": "shared-ui-general:build-storybook",
        "styles": ["libs/shared/ui/general/.storybook/tailwind-imports.css"],
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c libs/shared/ui/general/.storybook --url=http://localhost:4400"
      }
    }
  },
  "tags": []
}
